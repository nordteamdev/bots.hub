function Wo_open_send_gift(){
    $('#send_gift').modal('show');
}

function Wo_SendGiftToUser(from, to){
    var gift_id = $('#send-gift-button').attr('data-gift-id');
    var gift_img = $('#send-gift-button').attr('data-gift-img');

    if (!from || !to || !gift_id) {
        return false;
    }

    $('#send-gift-button').attr('disabled','');
    $.ajax({
        url: Wo_Ajax_Requests_File(),
        type: 'GET',
        dataType: 'json',
        data: {f: 'send_gift',from:from,to:to,gift_id:gift_id,gift_img:gift_img},
    })
    .done(function(data) {
        if (data.status == 200) {
            $('#send-gift-button').html('<?php echo $wo['lang']['gift_sent_succesfully']; ?>');
            Wo_Delay(function(){
                $("#send_gift").modal('hide');
                $('#send-gift-button').attr('data-gift-id', '');
                $('#send-gift-button').attr('data-gift-img', '');
                $('#send-gift-button').attr('disabled',false);
                $('#send-gift-button').html('<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-gift"><polyline points="20 12 20 22 4 22 4 12"></polyline><rect x="2" y="7" width="20" height="5"></rect><line x1="12" y1="22" x2="12" y2="7"></line><path d="M12 7H7.5a2.5 2.5 0 0 1 0-5C11 2 12 7 12 7z"></path><path d="M12 7h4.5a2.5 2.5 0 0 0 0-5C13 2 12 7 12 7z"></path></svg><?php echo $wo['lang']['send']; ?>');
            },1500);
        }
    })
    .fail(function() {
        console.log("error");
    })
    
}

function Wo_RegisterPoke(received_user_id , send_user_id){
    if (!received_user_id || !send_user_id) {
        return false;
    }
    $.ajax({
        url: Wo_Ajax_Requests_File(),
        type: 'GET',
        dataType: 'json',
        data: {f: 'poke',received_user_id:received_user_id,send_user_id:send_user_id},
    })
    .done(function(data) {
        if (data.status == 200) {
            $("#poke_modal").modal('show');
            $("#pokebutton").remove();
            Wo_Delay(function(){
                $("#poke_modal").modal('hide');
            },1500);
        }
    })
    .fail(function() {
        console.log("error");
    })
}

